				"@rollup/plugin-typescript": "8.2.1",
				"@types/node": "14.14.37",
				"@types/node-emoji": "1.8.1",
				"obsidian": "0.12.0",
				"rollup": "2.32.1",
				"tslib": "2.2.0",
				"typescript": "4.2.4"
		},
